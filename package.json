{
  "name": "rollup-plugin-remit",
  "description": "A rollup plugin that spawns rollup, emitting the output back into the main build.",
  "author": "Ian Purvis <ian@purvisresearch.com>",
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/remit.cjs",
  "exports": {
    "require": "./dist/remit.cjs",
    "import": "./dist/remit.mjs"
  },
  "types": "types/remit.d.ts",
  "files": [
    "dist",
    "types",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "node -r fs -e \"fs.rmdirSync('dist', { recursive: true })\"",
    "lint": "eslint .",
    "test": "ava",
    "simple-web-worker-example": "rollup -c -p \"serve={contentBase:'examples/simple-web-worker/dist'}\"",
    "complex-web-worker-example": "rollup -c -p \"serve={contentBase:'examples/complex-web-worker/dist'}\""
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-html": "^0.2.0",
    "ava": "^3.14.0",
    "eslint": "^7.15.0",
    "koa": "^2.13.0",
    "mime-types": "^2.1.27",
    "node-inspect": "^2.0.0",
    "puppeteer": "^5.5.0",
    "rollup": "^2.33.1",
    "rollup-plugin-serve": "^1.1.0"
  },
  "peerDependencies": {
    "rollup": "^2.0.0"
  }
}
